{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"kunwarf/multi-client",
            "hostname":"client",
            "essential": false
        },
        {
            "name":"server",
            "image":"kunwarf/multi-server",
            "hostname":"api",
            "essential": false
        },
        {
            "name":"worker",
            "image":"kunwarf/multi-worker",
            "hostname":"worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image":"kunwarf/multi-nginx",
            "hostname":"nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ]
        }
    ]
}